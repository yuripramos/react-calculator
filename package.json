{
  "name": "forecast-app",
  "version": "1.0.0",
  "description": "Forecast App",
  "main": "/src/App.js",
  "engines": {
    "node": "8.9.3"
  },
  "scripts": {
    "installation": "npm install",
    "test": "cross-env NODE_ENV=test && jest",
    "test:update": "npm run test -- -u",
    "test:coverage": "jest --coverage --runInBand",
    "start": "npm run dev",
    "lint": "eslint ./src --fix",
    "check": "npm run lint && npm run test",
    "start:server": "node ./src/index.js",
    "dev": "webpack-dev-server --port 3000 --inline --hot",
    "build": "cross-env NODE_ENV=production & webpack"
  },
  "author": "Yuri Ramos <yuri.pramos@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^5.2.0",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "3.3.0",
    "grid-styled": "^4.0.0",
    "history": "^4.7.2",
    "jest": "22.4.4",
    "jest-junit": "5.0.0",
    "jest-localstorage-mock": "2.2.0",
    "jest-mock-axios": "^2.1.11",
    "latest-version": "^3.1.0",
    "moment": "^2.24.0",
    "preact": "^8.1.0",
    "preact-compat": "^3.16.0",
    "prop-types": "^15.5.10",
    "querystringify": "^0.0.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.2.0",
    "redux-zero": "^4.13.6",
    "shortid": "^2.2.14",
    "sinon": "^7.2.3",
    "styled-components": "^3.4.9"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-styled-components": "1.3.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.26.1",
    "css-raw-loader": "^0.1.1",
    "eslint": "^4.13.1",
    "eslint-loader": "1.6.3",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^6.2.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^0.9.0",
    "help": "^3.0.2",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^9.12.0",
    "node-sass": "^4.3.0",
    "offline-plugin": "^4.6.2",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "path": "^0.12.7",
    "prettier": "^1.5.3",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "uglifyjs-webpack-plugin": "^0.4.1",
    "url-loader": "^0.5.9",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^1.16.2"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "<rootDir>/__mocks__/setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/components/**/styles.js",
      "!src/components/**/index.js",
      "!src/styles/**/*.js",
      "!src/services/history/*.js",
      "!src/app/index.js",
      "!src/services/*.js",
      "!src/components/common/Icon/*.js"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/shim.js",
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  }
}
